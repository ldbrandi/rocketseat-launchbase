{% extends 'layout.njk' %}

{% block content %}

    <div id="wrapper">
        <img src="{{about.avatar_url}}"
                alt="{{about.name}}">
        <h1>{{about.name}}</h1>
        <h2>{{about.title}}</h2>
        <p>{{about.description}}</p>
    </div>

    <div id="list">
        <h2>{{about.stack}}</h2>
        <ul>
        {% for icon in about.stack_icons %}

            <li>
                <img src="{{icon.url}}" alt="{{icon.name}}">
            </li>

        {% endfor %}
           
        </ul>
    </div>

    <div id="links-footer" class="links">
    {% for link in about.links %}

        <a href="{{link.url}}" target="_blank">{{link.name}}</a>

    {% endfor %}
    </div>

{% endblock content %}